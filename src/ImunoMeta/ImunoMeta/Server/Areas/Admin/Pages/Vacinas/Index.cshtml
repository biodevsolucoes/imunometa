@page
@model ImunoMeta.Server.Areas.Admin.Pages.Vacinas.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Vacina[0].Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vacina[0].Descricao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vacina[0].CssClass)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vacina[0].DataCadastro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vacina[0].DataCadastroUTC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Vacina[0].Removido)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Vacina) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descricao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CssClass)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataCadastro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataCadastroUTC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Removido)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
